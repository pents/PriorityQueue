<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <Title>C# simple PriorityQueue</Title>
        <Authors>Pent</Authors>
        <Company>Pents</Company>
        <PackageId>Pents.PQ</PackageId>
        <RepositoryUrl>https://github.com/pents/PriorityQueue</RepositoryUrl>
        <Description>Simple C# Priorityqueue implementation</Description>
        <PackageLicenseUrl>https://choosealicense.com/licenses/mit/</PackageLicenseUrl>
        <TargetFrameworks>netstandard2.0;netstandard2.1</TargetFrameworks>
        <PackageVersion>1.0.3</PackageVersion>
    </PropertyGroup>

    <PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
      <Optimize>false</Optimize>
    </PropertyGroup>

</Project>
